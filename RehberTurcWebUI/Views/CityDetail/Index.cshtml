@model City
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/Shared/_CityDetailLayout.cshtml";
}


<section class="listing-detail">
@await Component.InvokeAsync("_CityDetailListingHeaderComponentPartial",Model.Id)

@await Component.InvokeAsync("_CityDetailScrollMenuComponentPartial")
	<div class="listing-detail__content">
		<div class="container">
			<div class="row">
				<div class="col-lg-8">
					<div class="listing-detail__content-box">
@await Component.InvokeAsync("_CityDetailScrollMenuDescriptionComponentPartial", Model.Id)

@await Component.InvokeAsync("_CityDetailScrollGalleryComponentPartial", new {cityId = Model.Id} )

@await Component.InvokeAsync("_CityDetailScrollMenuTrendingComponentPartial")

@await Component.InvokeAsync("_CityDetailScrollMenuCommentComponentPartial", new { cityId = Model.Id })
					</div>
				</div>
					<div class="col-lg-4">
						<div class="sidebar">
	@await Component.InvokeAsync("_CityDetailProprietorProfileComponentPartial",Model.Id )
					     </div>
					</div>
			</div>
		</div>
	</div>
</section>

 @await Component.InvokeAsync("_CityDetailFooterComponentPartial")

